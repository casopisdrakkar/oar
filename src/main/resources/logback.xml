<configuration>
	<appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%d{HH:mm:ss.SSS} %msg%n
			</pattern>
		</encoder>
	</appender>
	
	<root level="INFO">
		<appender-ref ref="STDOUT"></appender-ref>
	</root>

	<logger name="sk.drakkar.oar.pipeline.Pipeline" level="WARN" />
</configuration>